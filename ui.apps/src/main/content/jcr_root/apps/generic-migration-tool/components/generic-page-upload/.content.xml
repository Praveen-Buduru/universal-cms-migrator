<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <image.webp/>
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="Excel to JSON Tool"
        sling:resourceType="/apps/generic-migration-tool/components/generic-page-upload/page.jsp"
        consoleId="excel-upload">
        <head jcr:primaryType="nt:unstructured">
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[granite.ui.coral.foundation]"/>
        </head>
        <title
            jcr:primaryType="nt:unstructured"
            jcr:title="Generic Page Exporter"
            sling:resourceType="granite/ui/components/shell/title"/>
        <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container"
            margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
                <columns
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}true">
                    <items jcr:primaryType="nt:unstructured">
                        <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                            <items jcr:primaryType="nt:unstructured">
                                <container
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                    <items jcr:primaryType="nt:unstructured">
                                        <heading
                                            granite:class="coral-Heading coral-Heading--2"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/heading"
                                            level="{Long}2"
                                            text="Universal CMS-to-AEM Migrator (UCM)"/>
                                        <well
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/well"
                                            margin="{Boolean}true">
                                            <items jcr:primaryType="nt:unstructured">
                                                <container
                                                    granite:id="fileUploadForm"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form"
                                                    enctype="multipart/form-data"
                                                    foundationForm="{Boolean}true"
                                                    loadingMask="{Boolean}true"
                                                    margin="{Boolean}true"
                                                    maximized="{Boolean}true"
                                                    style="vertical">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <fileUpload
                                                            granite:id="xls"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            autoStart="{Boolean}false"
                                                            emptyText="Choose XLS / XLSX File"
                                                            fieldLabel="Choose XLS / XLSX File"
                                                            icon="upload"
                                                            mimeTypes="[.xls,.xlsx]"
                                                            multiple="{Boolean}false"
                                                            name="file"
                                                            required="{Boolean}true"
                                                            rootPath="/content/dam"
                                                            text="Choose Json File for Single / Excel File for Multiple"/>
                                                        <container
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            margin="{Boolean}true">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <pathField
                                                                    granite:id="destPath"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    emptyText="Enter a path, e.g. /content/weretail/en"
                                                                    fieldLabel="Choose path to where pages should be created"
                                                                    required="{Boolean}true"
                                                                    rootPath="/content"/>
                                                                <assetField
                                                                    granite:id="assetsPath"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    emptyText="Enter the Assets path, e.g. /content/dam/weretail"
                                                                    fieldLabel="Choose Assets path"
                                                                    required="{Boolean}true"
                                                                    rootPath="/content/dam"/>
                                                                <templatePathField
                                                                    granite:id="templatePath"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    emptyText="Enter a path, e.g. /conf/wknd/settings/wcm/templates/content-page-template"
                                                                    fieldLabel="Choose Page Template"
                                                                    pickersrc="/mnt/overlay/wcm/core/content/sites/picker/content/selector.html?mode=selection&amp;amp;root=/conf&amp;amp;path=/conf&amp;amp;type=wcm-templates"
                                                                    required="{Boolean}true"
                                                                    rootPath="/conf"/>
                                                            </items>
                                                        </container>
                                                        <submitButton
                                                            granite:id="btnSubmit"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                                            text="Convert to AEM Pages"
                                                            type="submit"/>
                                                        <loadingGIF
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/field"
                                                            fieldLabel="\0"
                                                            wrapperClass="loading loading--hide"/>
                                                        <resultMessage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/field"
                                                            fieldLabel="\0"
                                                            wrapperClass="result"/>
                                                    </items>
                                                </container>
                                            </items>
                                        </well>
                                    </items>
                                </container>
                            </items>
                        </column>
                    </items>
                </columns>
            </items>
        </content>
    </jcr:content>
    <page.jsp/>
</jcr:root>
